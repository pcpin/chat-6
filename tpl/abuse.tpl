<PCPIN:TPL name="main">
<table id="abuse_data_table" class="tbl" border="0" cellspacing="1" cellpadding="0" width="100%" style="display:none">
  <tr>
    <td colspan="2" class="tbl_header_main">
      {LNG_ABUSE_ARRIVED}
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_DATE}:</b>
    </td>
    <td class="tbl_row">
      <span id="abuse_date"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_ROOM_NAME}:</b>
    </td>
    <td class="tbl_row">
      <span id="abuse_room"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_AUTHOR}:</b>
    </td>
    <td class="tbl_row">
      <span id="abuse_author"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_VIOLATION_CATEGORY}:</b>
    </td>
    <td class="tbl_row">
      <span id="abuse_category"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_ABUSER_NICKNAME}:</b>
    </td>
    <td class="tbl_row">
      <span id="abuse_abuser_nickname"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_VIOLATION_DESCRIPTION}:</b>
    </td>
    <td class="tbl_row" style="vertical-align:top">
      <span id="abuse_description"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row" colspan="2" style="text-align:center">
      <button type="button" id="enter_room_btn" title=""></button>
      &nbsp;
      <button type="button" title="{LNG_CLOSE_WINDOW}" onclick="window.close()">{LNG_CLOSE_WINDOW}</button>
    </td>
  </tr>
</table>
</PCPIN:TPL>