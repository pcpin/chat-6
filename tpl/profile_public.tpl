<PCPIN:TPL name="main">
<table class="tbl" border="0" cellspacing="1" cellpadding="0" width="100%" id="profile_table" style="display:none">
  <tr>
    <td colspan="2" class="tbl_header_main">
      <span id="profile_header"></span>
    </td>
  </tr>
  <tr id="avatars_row">
    <td colspan="2" class="tbl_row" style="text-align:center">
      <table border="0" cellspacing="0" cellpadding="3">
        <tr valign="top">
          <td align="right" id="avatar_image"></td>
          <td align="left" id="avatar_thumbs"></td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_NICKNAME}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_nickname"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_ONLINE_STATUS}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_online_status"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_REGISTRATION_DATE}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_registration_date"></span>
    </td>
  </tr>
  <tr>
    <td class="tbl_row">
      <b>{LNG_TIME_SPENT_ONLINE}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_time_spent_online"></span>
    </td>
  </tr>
  <tr id="profile_gender_row">
    <td class="tbl_row">
      <b>{LNG_GENDER}:</b>
    </td>
    <td class="tbl_row">
      <img src="./pic/clearpixel_1x1.gif" id="gender_image" border="0" />
    </td>
  </tr>
  <tr id="profile_age_row">
    <td class="tbl_row">
      <b>{LNG_AGE}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_age"></span>
    </td>
  </tr>
  <tr id="profile_email_row">
    <td class="tbl_row">
      <b>{LNG_EMAIL_ADDRESS}:</b>
    </td>
    <td class="tbl_row">
      <a href="mailto:" id="profile_email_link" title=""></a>
    </td>
  </tr>
  <tr id="profile_homepage_row">
    <td class="tbl_row">
      <b>{LNG_HOMEPAGE}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_homepage"></span>
    </td>
  </tr>
  <tr id="profile_icq_row">
    <td class="tbl_row">
      <b>{LNG_MESSENGER_ICQ}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_icq"></span>
    </td>
  </tr>
  <tr id="profile_msn_row">
    <td class="tbl_row">
      <b>{LNG_MESSENGER_MSN}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_msn"></span>
    </td>
  </tr>
  <tr id="profile_aim_row">
    <td class="tbl_row">
      <b>{LNG_MESSENGER_AIM}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_aim"></span>
    </td>
  </tr>
  <tr id="profile_yim_row">
    <td class="tbl_row">
      <b>{LNG_MESSENGER_YIM}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_yim"></span>
    </td>
  </tr>
  <tr id="profile_location_row">
    <td class="tbl_row">
      <b>{LNG_LOCATION}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_location"></span>
    </td>
  </tr>
  <tr id="profile_occupation_row">
    <td class="tbl_row">
      <b>{LNG_OCCUPATION}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_occupation"></span>
    </td>
  </tr>
  <tr id="profile_interests_row">
    <td class="tbl_row">
      <b>{LNG_INTERESTS}:</b>
    </td>
    <td class="tbl_row">
      <span id="profile_interests"></span>
    </td>
  </tr>
  <tr id="send_pm_button" style="display:none">
    <td class="tbl_row" colspan="2" style="text-align:center">
      <button id="invite_button" type="button">{LNG_INVITE}</button>
    </td>
  </tr>
</table>
<br />
<table border="0" cellspacing="0" cellpadding="0" width="100%" id="close_window_btn_tbl" style="display:none">
  <tr>
    <td style="text-align:center">
      <button type="button" title="{LNG_CLOSE_WINDOW}" onclick="window.close()">{LNG_CLOSE_WINDOW}</button>
    </td>
  </tr>
</table>
</PCPIN:TPL>